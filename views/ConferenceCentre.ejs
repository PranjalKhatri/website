<style>
    body {
        background: linear-gradient(to right, #ff9966, #ff5e62);
        /* background-image: url('path/to/image.jpg'); */
        /* background-repeat: no-repeat; */
        background-size: cover;
    }
    #a{
        /* background-color: #f2f2f2; */
        padding: 10px;
        border-radius: 10px;
        /* background-color: #ffcc00; */
        background: linear-gradient(to right,#ff9f66,#f75e63);
        margin: 5px;
        margin-top: 20px;
    }
    .container {
        display: grid;
        grid-template-columns: repeat(2, 1fr);
        background: linear-gradient(to right, #ff9966, #ff5e62);
        background-color: #f2f2f2;
        padding: 20px;
        margin-top: 20px; /* Added margin-top */
    }
    
    .time-column {
        background-color: #ffffff;
        padding: 10px;
        border-radius: 5px;
        margin: 10px;
    }
    </style>
    <title>Conference Centre</title>
<div id = 'a'>
<h1 style="text-align: center; color: #333; font-size: 24px; margin-bottom: 20px;">Bookings of Conference Centre</h1>
</div>
<div class="container">
    <% for (let i = 0; i < array.length; i+=2) { %>
        <div class="time-column">
            <p><%= array[i].dateTime %> to <%= array[i+1].dateTime %></p>
        </div>
    <% } %>
</div>